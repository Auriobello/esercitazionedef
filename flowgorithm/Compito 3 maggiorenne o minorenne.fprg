                                <output expression="&quot;L'eta inserita &#232; falsa&quot;" newline="True"/>
                                <output expression="&quot;Inserire l'et&#224; vera&quot;" newline="True"/>
                                <input variable="e"/>
                            </while>
                            <if expression="e&gt;=18">
                                <then>
                                    <output expression="&quot;Sei maggiorenne&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Sei minorenne&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Sei maggiorenne&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <if expression="e&lt;0 or e&gt;120">
                        <then>
                            <output expression="&quot;L'eta inserita &#232; falsa&quot;" newline="True"/>
                            <output expression="&quot;Inserire l'et&#224; vera&quot;" newline="True"/>
                            <input variable="e"/>
                            <while expression="e&lt;0 or e&gt;120">
                                <output expression="&quot;L'eta inserita &#232; falsa&quot;" newline="True"/>
                                <output expression="&quot;Inserire l'et&#224; vera&quot;" newline="True"/>
                                <input variable="e"/>
                            </while>
                            <if expression="e&gt;=18">
                                <then>
                                    <output expression="&quot;Sei maggiorenne&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Sei minorenne&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;Sei minorenne&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
