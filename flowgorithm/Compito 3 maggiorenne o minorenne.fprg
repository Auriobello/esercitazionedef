            <input variable="e"/>
            <if expression="e&gt;=18">
                <then>
                    <if expression="e&lt;0 or e&gt;120">
                        <then>
                            <output expression="&quot;L'eta inserita &#232; falsa&quot;" newline="True"/>
                            <output expression="&quot;Inserire l'et&#224; vera&quot;" newline="True"/>
                            <input variable="e"/>
