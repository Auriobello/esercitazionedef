    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="b, e, sum, i" type="Integer" array="False" size=""/>
            <output expression="&quot;Inserire la base&quot;" newline="True"/>
            <comment text="grave errore, con i numeri negativi fa robe a caso"/>
            <input variable="b"/>
            <assign variable="sum" expression="b"/>
            <output expression="&quot;Inserire l'esponente&quot;" newline="True"/>
            <input variable="e"/>
            <for variable="i" start="2" end="e" direction="inc" step="1">
                <assign variable="sum" expression="sum*b"/>
            </for>
            <output expression="&quot;La potenza di &quot; &amp; b &amp; &quot; alla &quot; &amp; e &amp; &quot; &#232; &quot; &amp; sum" newline="True"/>
        </body>
    </function>
